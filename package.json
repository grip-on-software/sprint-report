    "browser-sync-webpack-plugin": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "laravel-mix": "^2.1.11",
    "mustache-loader": "^1.4.0",
    "nyc": "^11.6.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
